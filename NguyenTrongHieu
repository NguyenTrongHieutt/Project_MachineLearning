   seq = seq.strip()\n    chunks = []\n\n    # cắt thành đoạn nhỏ\n    for i in range(0, len(seq), chunk_size):\n        sub = seq[i:i+chunk_size]\n\n        batch_labels, batch_strs, batch_tokens = batch_converter([(\"protein\", sub)])\n        batch_tokens = batch_tokens.to(\"cuda\")\n\n        with torch.no_grad():\n            result = esm_model(batch_tokens, repr_layers=[6], return_contacts=False)\n            rep = result[\"representations\"][6][0]\n\n        rep = rep[1:-1]  # remove CLS/EOS\n        chunks.append(rep.mean(0).cpu())\n\n    # trung bình embedding của tất cả chunk\n    final_emb = torch.stack(chunks, dim=0).mean(0)\n    return final_emb\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-12-08T04:21:57.116454Z","iopub.execute_input":"2025-12-08T04:21:57.116963Z","iopub.status.idle":"2025-12-08T04:21:57.122307Z","shell.execute_reply.started":"2025-12-08T04:21:57.116941Z","shell.execute_reply":"2025-12-08T04:21:57.121603Z"}},"outputs":[],"execution_count":21},{"cell_type":"code","source":"all_go_terms = [idx2go[i] for i in range(len(idx2go))]\n\nOUTPUT_FILE = \"submission.tsv\"\n\nwith open(OUTPUT_FILE, \"w\") as fout:\n\n    for protein_id, seq in test_sequences.items():\n        emb = embed_sequence(seq).to(\"cuda\")\n\n        with torch.no_grad():\n            probs = model(emb).sigmoid().cpu().numpy()\n\n        for go, p in zip(all_go_terms, probs):\n            if p > 0.01:\n                fout.write(f\"{protein_id}\\t{go}\\t{p:.3f}\\n\")\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-12-08T04:22:00.228451Z","iopub.execute_input":"2025-12-08T04:22:00.229161Z","iopub.status.idle":"2025-12-08T07:58:35.468484Z","shell.execute_reply.started":"2025-12-08T04:22:00.229126Z","shell.execute_reply":"2025-12-08T07:58:35.467876Z"}},"outputs":[],"execution_count":22}]}